---
import Head from "../components/Head.astro";

const { title } = Astro.props;
---

<!doctype html>
<html lang="zh">
    <head>
        <Head title = {title}/>
    </head>
	
    <body class="h-full w-full scroll-smooth text-neutral-800 dark:bg-neutral-900 dark:text-neutral-400">
        <slot />
    </body>
</html>
